
public class Skelett extends Monster {

	private String skelettName;
	
	private int angriff;
	
	private int verteidigung;
	
	private int leben;
	
	

	public Skelett(String skelettName, int angriff, int verteidigung, int leben) {
		super();
		this.skelettName = skelettName;
		this.angriff = angriff;
		this.verteidigung = verteidigung;
		this.leben = leben;
	}

	public String getSkelettName() {
		return skelettName;
	}

	public void setSkelettName(String skelettName) {
		this.skelettName = skelettName;
	}

	public int getAngriff() {
		return angriff;
	}

	public void setAngriff(int angriff) {
		this.angriff = angriff;
	}

	public int getVerteidigung() {
		return verteidigung;
	}

	public void setVerteidigung(int verteidigung) {
		this.verteidigung = verteidigung;
	}

	public int getLeben() {
		return leben;
	}

	public void setLeben(int leben) {
		this.leben = leben;
	}
	
	

}
